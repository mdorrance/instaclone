<%= render partial: 'layouts/feed_nav' %>
<div class="row">

  <div class="col-sm-6 col-sm-offset-3">
    <% current_user.client.user_media_feed.each do |feed| %>
    <div class="panel panel-default">
      <div class="panel-heading">
        <div class="row">
          <div class="col-sm-2">
            <%= image_tag feed.caption.from.profile_picture, class: "img-responsive img-circle",
                        alt: "#{feed.caption.from.username} image" %>
          </div>
          <div class="col-sm-8">
            <p class="text-left"><%= link_to feed.caption.from.username, "https://instagram.com/#{feed.caption.from.username}" %></p>
            <!-- <p class="text-left"><%= link_to feed.caption.from.username, "https://instagram.com/#{feed.caption.from.username}" %></p> -->
          </div>
          <div class="col-sm-2">
            <p>1 d</p>
          </div>
        </div>
      </div>
      <div class="panel-body">
        <%= image_tag feed.images.standard_resolution.url, class: "img-responsive",
                      alt: "#{current_user.name} image" %>
      </div>
      <div class="panel-footer">
        <% if feed.likes.count > 4 %>
        <p> <%= feed.likes.count %> likes</p>
        <% else %>
          <% feed.likes.data.each do |username| %>
          <%= link_to username.username, "https://instagram.com/#{username.username}" %>
          <% end %>
          like this
        <% end %>


        <p> <%= feed.caption.text %> </p>
        <hr>
        <div class="row">
          <div class="col-sm-1" id="heart">
            <i class="fa fa-heart-o fa-lg"></i>
          </div>
          <div class="col-sm-9" id="comment">
            <input type="text" class="form-control" placeholder="Comment ...">
          </div>
          <div class="col-sm-2">
            <div class="dots">
              <span>&#x25cb;</span><span>&#x25cb;</span><span>&#x25cb;</span>
            </div>
          </div>
        </div>
      </div>
    </div>
    <% end %>
  </div>
</div>
